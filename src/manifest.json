{
    "name": "UserId Replacer for GitHub",
    "description": "Display the OA ID instead of UserId for GitHub.",
    "version": "0.2.1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "js": [
                "index.bundle.js"
            ],
            "matches": [
                "https://github.com/*"
            ],
            "exclude_matches": [
            ]
        }
    ],
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "options_ui":{ 
        "page": "options.html"
    }
}
